{
    "name": "liyuze/laravel-method-chaining-macros",
    "description": "",
    "keywords": [
        "liyuze",
        "laravel-method-chaining-macros"
    ],
    "homepage": "https://github.com/liyuze/laravel-method-chaining-macros",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yuze Li",
            "email": "290315384@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "liyuze/method-chaining-proxy": "dev-ft-methodChaining"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Liyuze\\MethodChainingMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liyuze\\MethodChainingMacros\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MethodChainingMacrosServiceProvider"
            ],
            "aliases": {
                "LaravelMethodChainingMacros": "MethodChainingMacrosFacade"
            }
        }
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "../method-chaining-proxy"
        }
    }
}
